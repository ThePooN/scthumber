version: '3.4'

# Example purposes only, these are not default settings.
services:
  http:
    tty: true
    # See instructions if using a custom domain for dev.
    extra_hosts:
      - "osu-web.test:192.168.65.x"
    volumes:
      # use node_modules from the container, not the host.
      - node_modules:/app/node_modules

volumes:
  node_modules:
